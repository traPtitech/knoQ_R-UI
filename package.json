{
  "name": "knoq-r-ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "dev": "vite",
    "dev:mock": "VITE_BACKEND=mock vite",
    "prepare": "husky install",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "test": "vitest --ui --coverage.enabled=true",
    "generate": "openapi-typescript https://raw.githubusercontent.com/traPtitech/knoQ/master/docs/swagger.yaml -o src/lib/api-schema.d.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "date-fns": "^2.30.0",
    "markdown-it": "^13.0.2",
    "openapi-fetch": "^0.8.2",
    "pinia": "^2.1.7",
    "sass": "^1.69.6",
    "swrv": "^1.0.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.6.6",
    "@storybook/vue3-vite": "^7.6.6",
    "@types/eslint__eslintrc": "^2.1.1",
    "@types/eslint__js": "^8.42.3",
    "@types/markdown-it": "^13.0.7",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitejs/plugin-vue": "^4.6.0",
    "@vitest/coverage-v8": "^1.1.0",
    "@vitest/ui": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.19.2",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "msw": "^1.3.2",
    "openapi-typescript": "^6.7.3",
    "openapi-typescript-helpers": "^0.0.6",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.6",
    "typescript": "^5.2.2",
    "vite": "^4.5.1",
    "vitest": "^1.1.0",
    "vue-eslint-parser": "^9.3.2",
    "vue-tsc": "^1.8.27"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
